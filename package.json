{
  "name": "microservicio-rips-minsalud",
  "version": "1.0.0",
  "description": "Microservicio para envío automático de archivos RIPS al Ministerio de Salud de Colombia",
  "main": "index.js",
  "scripts": {
    "dashboard": "node src/services/dashboard-server.js",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "manual": "cross-env MODO_EJECUCION=manual node index.js",
    "continuo": "cross-env MODO_EJECUCION=continuo node index.js",
    "programado": "cross-env MODO_EJECUCION=programado node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration"
  },
  "keywords": [
    "rips",
    "minsalud",
    "colombia",
    "microservicio",
    "salud",
    "sispro"
  ],
  "author": "LorveOne",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^30.0.0",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
